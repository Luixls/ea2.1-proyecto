<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <title>Actividades de la Semana</title>
</head>
<body>
    <h1>Actividades de la Semana: <%= semana %> (<%= inicioSemana %> - <%= finSemana %>)</h1>
    <h2>Trimestre: <%= trimestre %></h2>
    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>Nombre del Evento</th>
                <th>Fecha</th>
                <th>Día de la Semana</th>
                <th>Materia</th>
                <th>Profesor</th>
                <th>Sección</th>
            </tr>
        </thead>
        <tbody>
            <% actividades.forEach(function(actividad) { %>
                <tr>
                    <td><%= actividad.ID %></td>
                    <td><%= actividad.Nombre %></td>
                    <td><%= actividad.Fecha.fecha %></td>
                    <td><%= actividad.Fecha.diaSemana %></td>
                    <td><%= actividad.Materia %></td>
                    <td><%= actividad.Profesor %></td>
                    <td><%= actividad.Seccion %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
